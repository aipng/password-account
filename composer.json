{
	"name": "aipng/password-account",
	"description": "Password account management - standardize basic password handling for user accounts",
	"type": "library",
	"license": "MIT",
	"authors": [
		{
			"name": "Karel Horsky",
			"email": "karel.horsky@centrum.cz"
		}
	],
	"autoload": {
		"classmap": [
			"src/"
		]
	},
	"autoload-dev": {
		"classmap": [
			"tests"
		]
	},
	"require": {
		"php": ">=7.1",
		"nette/security": "^2.4.2",
		"nette/utils": "^2.4.7",
		"symfony/event-dispatcher": "^3.3.5",
		"kdyby/datetime-provider": "^1.0"
	},
	"require-dev": {
		"nette/tester": "^2.3",
		"mockery/mockery": "^1.0",
		"nette/di": "^2.4.9"
	},
	"suggest": {
		"nette/di": "Needed when using SecurityExtension for registering services to application"
	},
	"minimum-stability": "stable"
}
